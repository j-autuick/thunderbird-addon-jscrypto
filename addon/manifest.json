{
    "manifest_version": 2,
    "name": "Crypto add-on",
    "description": "A simple password AES cryptographic addon",
    "version": "1.0",
    "author": "Esteban Licea",
    "applications": {
        "gecko": {
            "id": "esteban@little-beak.com",
            "strict_min_version": "78.0"
        }
    },  
    "background": {
        "scripts": [
          "background.js"
        ]
      },
    "compose_action": {
        "default_popup": "messagePopup/popup.html",
        "default_title": "Insecure",
        "default_icon": "images/unlocked_64px.png"
    },
    "permissions": [
        "compose"
    ],
    "icons": {
        "64": "images/unlocked_64px.png",
        "32": "images/unlocked_32px.png",
        "16": "images/unlocked_16px.png"
    }
}